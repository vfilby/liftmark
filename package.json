{
  "name": "liftmark2",
  "version": "1.0.26",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --audit-level=high",
    "ci": "npm run audit && npm run typecheck && npm run test:coverage",
    "prerelease:check": "node -e \"const { execSync } = require('child_process'); const branch = execSync('git branch --show-current', { encoding: 'utf8' }).trim(); if (branch !== 'main') { console.error('Error: Must be on main branch for releases. Current branch:', branch); process.exit(1); } console.log('âœ… On main branch - ready for release');\"",
    "release:alpha": "npm run prerelease:check && node scripts/release-helper.js alpha",
    "release:beta": "npm run prerelease:check && node scripts/release-helper.js beta",
    "release:production": "npm run prerelease:check && node scripts/release-helper.js production",
    "release:cleanup:alpha": "node scripts/cleanup-release.js alpha",
    "release:cleanup:beta": "node scripts/cleanup-release.js beta",
    "release:cleanup:production": "node scripts/cleanup-release.js production"
  },
  "dependencies": {
    "@kingstinct/react-native-healthkit": "^13.1.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "date-fns": "^4.1.0",
    "expo": "~54.0.31",
    "expo-audio": "~1.1.1",
    "expo-background-fetch": "^14.0.9",
    "expo-cloudkit": "file:./modules/expo-cloudkit",
    "expo-constants": "^18.0.12",
    "expo-crypto": "^15.0.8",
    "expo-document-picker": "~14.0.8",
    "expo-keep-awake": "^15.0.8",
    "expo-linking": "^8.0.11",
    "expo-live-activity": "^0.4.2",
    "expo-router": "~6.0.22",
    "expo-sharing": "~14.0.8",
    "expo-sqlite": "^16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "^14.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-test-renderer": "19.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@expo/cli": "^54.0.20",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~54.0.10",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
